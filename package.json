{
  "name": "angular-sdk",
  "version": "0.0.0",
  "repository": "https://bitbucket.org/solidgate/angular-sdk",
  "scripts": {
    "ng": "ng",
    "start": "ng serve @solidgate/example",
    "build": "ng build",
    "build:sdk": "ng build @solidgate/angular-sdk",
    "serve:example": "ng serve @solidgate/example",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "publish": "npm i && npm run build:sdk && cd ./dist/solidgate/angular-sdk && npm publish"
  },
  "private": true,
  "dependencies": {
    "@solidgate/client-sdk-loader": "^1.6.2",
